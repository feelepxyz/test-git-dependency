{
  "name": "test-git-dependency",
  "version": "1.0.13",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/feelepxyz/test-git-dependency.git"
  },
  "scripts": {
    "test": "npm run clean && npm run build && mocha test --require babel-core/register",
    "clean": "rm -rf dist && rm -rf test/*package*/node_modules && git checkout test/*package*/package.json || exit 0",
    "build": "babel --copy-files -d dist/ src/",
    "prepare": "npm run build"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "inquirer": "3.0.6",
    "minimist": "1.2.0",
    "node-fetch": "1.6.3",
    "babel-polyfill": "6.23.0",
    "opn": "4.0.2"
  }
}
